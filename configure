if test "${ac_cv_prog_ISSED+set}" = set; then
  echo $ECHO_N "(cached) $ECHO_C" >&6
else
  if test -n "$ISSED"; then
  ac_cv_prog_ISSED="$ISSED" # Let the user override the test.
else
as_save_IFS=$IFS; IFS=$PATH_SEPARATOR
for as_dir in $PATH
do
  IFS=$as_save_IFS
  test -z "$as_dir" && as_dir=.
  for ac_exec_ext in '' $ac_executable_extensions; do
  if $as_executable_p "$as_dir/$ac_word$ac_exec_ext"; then
    ac_cv_prog_ISSED="yes"
    echo "$as_me:$LINENO: found $as_dir/$ac_word$ac_exec_ext" >&5
    break 2
  fi
done
done

  test -z "$ac_cv_prog_ISSED" && ac_cv_prog_ISSED="no"
fi
fi
ISSED=$ac_cv_prog_ISSED
if test -n "$ISSED"; then
  echo "$as_me:$LINENO: result: $ISSED" >&5
echo "${ECHO_T}$ISSED" >&6
else
  echo "$as_me:$LINENO: result: no" >&5
echo "${ECHO_T}no" >&6
fi

