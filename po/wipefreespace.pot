#: src/wipefreespace.c:708
msgid " [options] "
msgstr ""

#: src/wipefreespace.c:710
msgid ""
"Options:\n"
"--all-zeros\t\tUse only zeros for wiping\n"
"--background\t\tContinue work in the background, if possible\n"
"-b|--superblock <off>\tSuperblock offset on the given filesystems\n"
"-B|--blocksize <size>\tBlock size on the given filesystems\n"
"-f|--force\t\tWipe even if the file system has errors"
msgstr ""

#: src/wipefreespace.c:717
msgid ""
"-h|--help\t\tPrint help\n"
"-n|--iterations NNN\tNumber of passes (>0, default: 35 in Gutmann method, 25 "
"in random)\n"
"--last-zero\t\tPerform additional wiping with zeros\n"
"-l|--license\t\tPrint license information\n"
"--nopart\t\tDo NOT wipe free space in partially used blocks"
msgstr ""

#: src/wipefreespace.c:723
msgid ""
"--nounrm\t\tDo NOT wipe undelete information\n"
"--nowfs\t\t\tDo NOT wipe free space on file system\n"
"--use-ioctl\t\tDisable device caching during work (can be DANGEROUS)\n"
"-v|--verbose\t\tVerbose output\n"
"-V|--version\t\tPrint version number"
msgstr ""
